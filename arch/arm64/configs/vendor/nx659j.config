# Charging
CONFIG_NUBIA_CHARGE_FEATURE=y
CONFIG_NUBIA_MSOC_FEATURE=y

# Device specific config
CONFIG_MACH_NUBIA_NX659J=y

# Device Tree
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# Fan
CONFIG_FAN=y
CONFIG_FAN_CORE=y

# Fbdev
CONFIG_FB=y
CONFIG_FB_ARMCLCD=y

# Kernel Image
CONFIG_BUILD_ARM64_UNCOMPRESSED_KERNEL=n

# Leds
CONFIG_NUBIA_LED=y
CONFIG_NUBIA_LED_AW22XXX=y

# Local config
CONFIG_LOCALVERSION="-LineageOS"

# MSM 11AD
CONFIG_MSM_11AD=n
CONFIG_WIL6210=n

# NFC
CONFIG_NFC_NQ=n
CONFIG_NFC=y
CONFIG_SEC_NFC=y

# Pstore
CONFIG_PSTORE=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y

# Sensors
CONFIG_NUBIA_SENSORS=y
CONFIG_NUBIA_SENSORS_STK3337=y

# Speculative page fault
CONFIG_SPECULATIVE_PAGE_FAULT=n

# Sound
CONFIG_SND_SOC_MAX98937=y

# Timer frequency
CONFIG_HZ_300=y

# Touchscreen
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX=n
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_NUBIA=y

# USB
CONFIG_NUBIA_DP=y
CONFIG_NUBIA_USB2_DOCK_SWITCH=y

# Vibrator
CONFIG_INPUT_QTI_HAPTICS=n
CONFIG_AW8695_HAPTIC=y

# WiFi
CONFIG_QCA_CLD_WLAN=y
CONFIG_CNSS_QCA6390=y
